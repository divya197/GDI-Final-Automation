package com.gdi.functionality;

import io.appium.java_client.android.AndroidDriver;

public class FuctionalityMapper {
	
	public static void executeFunction(String functionName, AndroidDriver driver){
		if(action.equalsIgnoreCase("click"))
		{
		lib=new keywordlib();
		data=lib.click(driver, input1, action,i,testReport);
		}
		if(action.equalsIgnoreCase("jseclick"))
		{
		lib=new keywordlib();
		data=lib.jseClick(driver, input1, action, i, testReport);
		}
		if(action.equalsIgnoreCase("clickonbutton"))
		{
		keywordlib.clickOnButton(driver, input1, action, i, testReport);
		}
	}
}
